<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.api.estate.api.common.mapper.DistrictMapper">

    <resultMap id="districtInfo" type="com.api.estate.api.common.vo.DistrictVo">
        <result column="매물코드" property="memulcode" jdbcType="VARCHAR"/>
    </resultMap>

    <select id="getDistrictList" resultMap="districtInfo">
        select 매물코드 from modubiz.매물_기본정보 limit 5;
    </select>

</mapper>
